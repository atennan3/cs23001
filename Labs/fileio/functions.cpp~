#include <iostream>
#include <fstream>


#include "functions.hpp"

void readUntilSpace() {
  std::ifstream file ("fileio-data-1.txt");
  if(!file.is_open()) {
    std::cout << "Unable to open file\n";
    return;
  }
    
  char ch;
  while (file >> ch) {
    std::cout << ch;
  }
}
